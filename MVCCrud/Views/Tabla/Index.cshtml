@model List<MVCCrud.Models.ViewModels.ListTablaViewModel>
@{
    ViewBag.Title = "Datos de vehiculos";
}

<h2>@ViewBag.Title</h2>

<div class="row" style="text-align:right;padding:5px;">
    <a href="~/Tabla/Nuevo" class="btn btn-primary">Nuevo</a>
</div>
<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>Id</th>
                <th>Patente</th>
                <th>Marca</th>
                <th>Modelo</th>
                <th>Puertas</th>
                <th>Dueño</th>
                <th></th>
            </tr>
            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.Id</td>
                    <td>@oElemento.Patente</td>
                    <td>@oElemento.Marca</td>
                    <td>@oElemento.Modelo</td>
                    <td>@oElemento.Puertas</td>
                    <td>@oElemento.Dueño</td>
                   
                </tr>

            }
        </table>
    </div>

</div>

